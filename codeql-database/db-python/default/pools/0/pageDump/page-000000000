name: U ... review)- uses: ... kout@v4runs-on ... -latestsecurity:name: S ... canning/Users/frank/RouteForceRouting/.github/workflows/advanced-project-automation.ymlAdvanced Project AutomationAdvance ... omationissuestypesopenedlabeledunlabeledassigned[opened ... signed]types:  ... signed]issue_commentcreated[created]types: [created]issues:auto-triageactions/github-script@v7actions ... ript@v7scriptconst issue = context.payload.issue;
const comment = context.payload.comment;

// Auto-assign based on component labels
const componentAssignments = {
  'component:backend': 'ApacheEcho',
  'component:frontend': 'frontend-dev',
  'component:api': 'api-team'
};

// Check for blocking issues
if (comment && comment.body.includes('blocked by #')) {
  await github.rest.issues.addLabels({
    owner: context.repo.owner,
    repo: context.repo.repo,
    issue_number: issue.number,
    labels: ['blocked']
  });
}

// Auto-prioritize based on keywords
if (issue.title.toLowerCase().includes('critical') || 
    issue.title.toLowerCase().includes('urgent')) {
  await github.rest.issues.addLabels({
    owner: context.repo.owner,
    repo: context.repo.repo,
    issue_number: issue.number,
    labels: ['high-priority']
  });
}
