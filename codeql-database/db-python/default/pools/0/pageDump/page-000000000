  echo " Trivy found security issues, but continuing workflow..."
  # Create minimal SARIF file if scan failed completely
  echo '{"version":"2.1.0","runs":[{"tool":{"driver":{"name":"trivy","version":"unknown"}},"results":[]}]}' > trivy-results.sarif
}
