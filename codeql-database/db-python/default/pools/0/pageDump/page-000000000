${{ runner.os }}-node-${{ hashFiles('**/package-lock.json', '**/yarn.lock') }}${{ run ... ck') }}path: |name: C ... e dirs)Install and build all frontend applicationsInstall ... cations# Function to install and build a frontend directory
install_and_build() {
  local dir=$1
  local name=$2
  
  if [ -d "$dir" ] && [ -f "$dir/package.json" ]; then
    echo " $name directory found, installing dependencies..."
    cd "$dir"
    
    # Clear npm cache to prevent enoent errors
    npm cache clean --force || echo "Cache clean failed, continuing..."
    
    # Remove node_modules and package-lock.json to prevent conflicts
    rm -rf node_modules package-lock.json || echo "No existing modules to remove"
    
    # Install fresh dependencies
    npm install --prefer-offline --no-audit --legacy-peer-deps || {
      echo " $name npm install failed, trying with different flags..."
      npm install --no-audit --legacy-peer-deps --force || {
        echo " All $name npm install attempts failed, skipping $name build"
        cd ..
        return 0
      }
    }
    
    # Build the application
    npm run build || {
      echo " $name build failed, but continuing deployment..."
      cd ..
      return 0
    }
    echo " $name built successfully"
    cd ..
  else
    echo "$name directory not found or missing package.json, skipping $name build"
  fi
}

# Install and build all frontend applications
install_and_build "frontend" "Frontend"
install_and_build "client" "Client"
install_and_build "mobile/pwa" "Mobile PWA"
