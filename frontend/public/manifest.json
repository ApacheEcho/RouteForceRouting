  "categories": ["productivity", "developer", "utilities"],
  "screenshots": [
    {
      "src": "/screenshots/voice-dashboard.png",
      "sizes": "1280x720",
      "type": "image/png",
      "label": "Voice Dashboard"
    },
    {
      "src": "/screenshots/voice-coding.png",
      "sizes": "1280x720",
      "type": "image/png",
      "label": "Voice Coding Interface"
    }
  ],
  "shortcuts": [
    {
      "name": "Voice Notes",
      "short_name": "Notes",
      "description": "Quick voice note capture",
      "url": "/voice-dashboard?tab=notes",
      "icons": [
        {
          "src": "/icons/notes-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Voice Commit",
      "short_name": "Commit",
      "description": "Voice-dictated commits",
      "url": "/voice-dashboard?tab=commit",
      "icons": [
        {
          "src": "/icons/commit-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Voice Coding",
      "short_name": "Code",
      "description": "Voice-driven code generation",
      "url": "/voice-dashboard?tab=coding",
      "icons": [
        {
          "src": "/icons/code-96x96.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://app.routeforcepro.com"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "voice",
      "url": "/voice-dashboard?voice=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/voice-dashboard",
      "accept": {
        "audio/*": [".wav", ".mp3", ".ogg", ".webm", ".m4a"]
      }
    }
  ]
}