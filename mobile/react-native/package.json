{
  "name": "routeforce-mobile",
  "version": "1.0.0",
  "description": "RouteForce Mobile Application - Driver and Route Management",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace RouteForce.xcworkspace -scheme RouteForce -configuration Release",
    "clean": "react-native clean-project-auto",
    "pod-install": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.2.0",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-navigation/bottom-tabs": "^7.4.4",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/stack": "^7.4.4",
    "@reduxjs/toolkit": "^2.0.1",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "^0.80.2",
    "react-native-background-job": "^2.3.1",
    "react-native-background-timer": "^2.4.1",
    "react-native-biometrics": "^3.0.1",
    "react-native-camera": "^4.2.1",
    "react-native-device-info": "^14.0.4",
    "react-native-fast-image": "^8.6.3",
    "react-native-fs": "^2.20.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-keychain": "^10.0.0",
    "react-native-maps": "^1.8.0",
    "react-native-orientation-locker": "^1.6.0",
    "react-native-permissions": "^5.4.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-reanimated": "^4.0.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-share": "^12.1.1",
    "react-native-sound": "^0.12.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-tts": "^4.1.0",
    "react-native-uuid": "^2.0.1",
    "react-native-vector-icons": "^10.0.2",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.80.2",
    "@react-native/metro-config": "^0.80.2",
    "@react-native/typescript-config": "^0.80.2",
    "@types/lodash": "^4.14.202",
    "@types/react": "^19.1.8",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "babel-jest": "^30.0.5",
    "detox": "^20.13.0",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": "@react-native"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}
